@page
@model _5.Pages.Razor.SecureModel
@{
}

<h1>This is the secure razor page with default policy</h1>

@if(User.Identity.IsAuthenticated)
{
    <table>
        <thead>
            <tr>
                <th>Type</th>
                <th>Value</th>
            </tr>
        </thead>
        <tbody>
            @foreach(var claim in User.Claims)
            {
                <tr>
                    <td>@claim.Type</td>
                    <td>@claim.Value</td>
                </tr>
            }
        </tbody>
    </table>
}